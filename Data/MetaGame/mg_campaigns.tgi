	[Kingdom Template=GreatBritainKingdomTemplate]
	mgai_ids = gbr_worldmap_mgai
		
	[Kingdom Template=GermanyKingdomTemplate]
	mgai_ids = ger_worldmap_mgai
		
	[Kingdom Template=RussiaKingdomTemplate]
	mgai_ids = rus_worldmap_mgai	

