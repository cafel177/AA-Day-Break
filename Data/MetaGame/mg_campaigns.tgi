[DynamicCampaign Template=DefaultDynamicCampaignTemplate]
{
	;; General
	ids = jpn_dynamic_campaign
	name = "#mg_campaigns_jpn_dynamic_campaign_name"
	default_kingdom = kingdom_jpn
	cinematic_intro = /Video/Metagame/JPNIntro.bik
	choose_background_texture = DynamicCampaigns/%s/JPNBackground.png
	description = "#mg_campaigns_jpn_dynamic_campaign_description"
		
	;; Map
	map_ids = worldmap
	map_table_ids = worldmaptable

	[ChooseButtonVE]
	when = SELECTED + UNFOCUSED
	texture = DynamicCampaigns/%s/JPNCampaignPressed.png

	[ChooseButtonVE]
	when = UNSELECTED + UNFOCUSED
	texture = DynamicCampaigns/%s/JPNCampaign.png

	[ChooseButtonVE]
	when = SELECTED + FOCUSED
	texture = DynamicCampaigns/%s/JPNCampaignPressedFocused.png

	[ChooseButtonVE]
	when = UNSELECTED + FOCUSED
	texture = DynamicCampaigns/%s/JPNCampaignFocused.png

	[Kingdom Template=JapanKingdomTemplate]
	mgai_ids = jpn_worldmap_mgai
	
	[Kingdom Template=UnitedStatesKingdomTemplate]
