[Template AAUnitVehicleSPA Template=AAUnitVehicle]
{
	[ElementComponent]
	morale_value = 15
	group_priority = 2

	[PropertiesComponent]
	{
		[Property]
		property_ids = artillery

		[Property]
		property_ids = anti_stun1
	}

	[View]
	{		
		[Swivel]
		{
			swivel_node_ids = base
			swivel_inertia = .25
			spring_constant = .95
			swivel_mass = 8000
			wind_sway = false
			recoil = true
			impact_sway = true
			movement_inertia = false
		}
	}


	[MoverComponent]
	{
		collision_impulse = 8000
		acceleration = 1
		turn_radius = 0.9
		turn_type = in_place

		[WhenStartMoving Template=FXC_PlaySFX]
		audio_ids = tank_start
		
		[WhenStopMoving Template=FXC_PlaySFX]
		audio_ids = tank_stop

		[WhenStartMoving template=FXC_CreateVFX]
		vfx_ids = move_effect_tank_medium
		location = base
	}
	
	[BodyComponent]
	{
		[WhenHealth template=FXC_CreateVFX]
		{
			vfx_ids = medium_tank_debris
			location = explosion_point_death01
			health = 0
			health_trigger_type = ABSOLUTE_BELOW
		}
	}
	
	[PerformerComponent]
	{
		[Turret]
		{
			turret_ids = Turret
			yaw_rotation_per_sec_degrees = 360
		}
	}

	[Size]
	{

		;; drp011504 - changed radius from 1.4 to 0.5
		blocking_shape = CIRCLE
		blocking_radius = 0.8
					
		soft_shape = RECT
		rect_width = 1.6
		rect_height = 3.0
	}
}

