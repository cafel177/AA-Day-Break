[Template AAUnit Template=Unit]
{
	[ElementComponent]
	morale_value = 5
	resupply_rate = 0.15
	
	[DetectionComponent]
	detection_range = 18.0
	remote_detection_range = 10.0
	
	[BodyComponent]
	defense_icon = /Properties/AbilityIcons/IconAbilityDefense.png
	
	[EconomyComponent]
	{
		[Cost]
		
		[Upkeep]
		
		[Production]
	}
	
	[View]
	shadow_texture = /effects/shadows/UnitShadow.dds
}

[Template AAAmbientUnit Template=AAUnit]
{
	[BodyComponent]
	health = 300
	defense = 1
	
	[Flags]
	{
		blocking = false
		ownable = false
		targetable = false
		invulnerable = true
		radar = false
	}

	[ElementComponent]
	morale_value = 0

	[MoverComponent]
	turn_radius = 0.5
	turn_type = WHILE_MOVING
	speed_max = 1
	
	[Placement]
	placement_group_IDS = group_ambient

	[PropertiesComponent]
	{
		[Property]
		property_IDS = front
	}

	[Size]
	{
		blocking_shape = CIRCLE
		blocking_radius = 0.5
		
		soft_shape = CIRCLE
		soft_radius = 0.75
	}
}

[Template AAUnitVehicle Template=AAUnit]
{
	places_tracks = true

	[Flags]
