                }

                [WhenCreated template=FXC_CreateVFX]
                {
                	VFX_IDS = taillight_01
                	attached = true
                	location = taillight_ae_01
                }

                [WhenCreated template=FXC_CreateVFX]
                {
                	VFX_IDS = taillight_01
                	attached = true
                	location = taillight_ae_02
                }

                [WhenCreated template=FXC_CreateVFX]
                {
                	VFX_IDS = headlight_projection
